[Service]
Type=simple
RemainAfterExit=true
StandardOutput=journal
ExecStartPre=/usr/bin/distrobox-rm -f ephem-arch
ExecStart=/usr/bin/bash -c 'distrobox-create --yes -i quay.io/toolbx/arch-toolbox:latest --additional-flags "-v /var/home/linuxbrew:/var/home/linuxbrew -v /home/linuxbrew:/home/linuxbrew" --name ephem-arch && distrobox-enter ephem-arch -- echo started' 
ExecStop=/usr/bin/distrobox-rm -f ephem-arch

[Install]
WantedBy=default.target

