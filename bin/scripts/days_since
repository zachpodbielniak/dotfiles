#!/bin/python3
from datetime import datetime
from sys import argv, exit


def main(prev_date: str) -> None:
    today: datetime = datetime.today()
    prev: datetime = datetime.strptime(prev_date, "%Y-%m-%d")
    diff: int = (today - prev).days 
    print(f"{diff}")



if (__name__ == '__main__'):
    if (len(argv) < 2):
        print(f"Usage: {argv[0]} <date:%Y-%m-%d>")
        exit(1)

    main(argv[1])

